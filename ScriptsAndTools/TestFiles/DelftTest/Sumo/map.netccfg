<?xml version="1.0" encoding="UTF-8"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/netconvertConfiguration.xsd">

    <input>
        <osm-files value="C:\Local\Berend\PWS\Sumo\TUDelft/map_bbox.osm.xml"/>
    </input>

    <output>
        <output-file value="C:\Local\Berend\PWS\Sumo\TUDelft\map.net.xml"/>
        <output.street-names value="true"/>
        <output.original-names value="true"/>
    </output>

    <tls_building>
        <tls.discard-simple value="true"/>
        <tls.join value="true"/>
        <tls.guess-signals value="true"/>
    </tls_building>

    <ramp_guessing>
        <ramps.guess value="true"/>
    </ramp_guessing>

    <edge_removal>
        <remove-edges.by-vclass value="tram,rail_urban,rail_electric,bicycle,pedestrian"/>
    </edge_removal>

    <processing>
        <geometry.remove value="true"/>
        <roundabouts.guess value="true"/>
        <junctions.join value="true"/>
        <junctions.corner-detail value="5"/>
    </processing>

    <report>
        <verbose value="true"/>
    </report>

</configuration>
